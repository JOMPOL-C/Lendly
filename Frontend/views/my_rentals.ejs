                <script>
                    function openTab(event, tabId, el) {
                        event.preventDefault();
                        document.querySelectorAll('.tab-content').forEach(div => div.style.display = 'none');
                        document.querySelectorAll('.tabs a').forEach(a => a.classList.remove('active'));
                        document.getElementById(tabId).style.display = 'block';
                        el.classList.add('active');
                    }
                </script>


                <script>
                    document.querySelectorAll(".countdown").forEach(span => {
                        const startTime = Number(span.dataset.time);
                        function update() {
                            const remain = 30 * 60 * 1000 - (Date.now() - startTime);
                            if (remain <= 0) { span.textContent = "‡∏´‡∏°‡∏î‡πÄ‡∏ß‡∏•‡∏≤‡πÅ‡∏•‡πâ‡∏ß"; return; }
                            const min = Math.floor(remain / 60000);
                            const sec = Math.floor((remain % 60000) / 1000);
                            span.textContent = `${min}:${sec.toString().padStart(2, '0')} ‡∏ô‡∏≤‡∏ó‡∏µ`;
                            requestAnimationFrame(update);
                        }
                        update();
                    });
                </script>
